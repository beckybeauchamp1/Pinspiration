<h2>Edit Board</h2>

<%= form_for @board do |f| %>

<%= f.label :name %>
<%= f.text_field :name %>

<%= f.label :description %>
<%= f.text_field :description %>

<%= f.label :photo_url %>
<%= f.text_field :photo_url %>

<%= f.submit %>
<% end %>

<%= form_for @board, method: :delete do |f| %>
<%= f.submit "Delete!", class: "delete" %>
<% end %>
